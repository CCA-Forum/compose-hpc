install-nobase_pyexecLTLIBRARIES: $(nobase_pyexec_LTLIBRARIES)
	@$(NORMAL_INSTALL)
	test -z "$(pyexecdir)" || $(MKDIR_P) "$(DESTDIR)$(pyexecdir)"
	@list='$(nobase_pyexec_LTLIBRARIES)'; test -n "$(pyexecdir)"  || list=; \
	for p in $$list; do if test -f "$$p"; then echo "$$p $$p"; else :; fi; done | \
	sed '/ .*\//!s/ .*/ ./; s,\( .*\)/[^/]*$$,\1,' | \
	$(AWK) 'BEGIN { cur = "." } \
	  { if ($$2 == cur) { files = files " " $$1 } \
	    else { print cur, files; files = $$1; cur = $$2 } } \
	  END { print cur, files }' | \
	while read dir files; do \
	  test -z "$$files" || { \
	    test "x$$dir" = x. || { \
	      echo "$(MKDIR_P) '$(DESTDIR)$(pyexecdir)/$$dir'"; \
	      $(MKDIR_P) "$(DESTDIR)$(pyexecdir)/$$dir"; }; \
	    echo " $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=install $(INSTALL) $(INSTALL_STRIP_FLAG) $$files '$(DESTDIR)$(pyexecdir)/$$dir'"; \
	    $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=install $(INSTALL) $(INSTALL_STRIP_FLAG) $$files "$(DESTDIR)$(pyexecdir)/$$dir" || exit $$?; \
	  }; \
	done

uninstall-nobase_pyexecLTLIBRARIES:
	@$(NORMAL_UNINSTALL)
	@list='$(nobase_pyexec_LTLIBRARIES)'; test -n "$(pyexecdir)" || list=; \
	for p in $$list; do \
	  f=$$p; \
	  echo " $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=uninstall rm -f '$(DESTDIR)$(pyexecdir)/$$f'"; \
	  $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=uninstall rm -f "$(DESTDIR)$(pyexecdir)/$$f"; \
	done

clean-nobase_pyexecLTLIBRARIES:
	-test -z "$(nobase_pyexec_LTLIBRARIES)" || rm -f $(nobase_pyexec_LTLIBRARIES)
	@list='$(nobase_pyexec_LTLIBRARIES)'; \
	locs=`for p in $$list; do echo $$p; done | \
	      sed 's|^[^/]*$$|.|; s|/[^/]*$$||; s|$$|/so_locations|' | \
	      sort -u`; \
	test -z "$$locs" || { \
	  echo rm -f $${locs}; \
	  rm -f $${locs}; \
	}
